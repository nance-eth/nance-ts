{
  "name": "nance",
  "version": "1.0.0",
  "description": "automated governance pipeliner",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV='prod' && export CONFIG='juicebox' && ts-node src/index.ts",
    "dev": "export LOG_LEVEL=silly && nodemon src/index.ts",
    "update-start": "pm2 stop all && git pull && pm2 start process.json",
    "test": "export NODE_ENV='dev' && jest",
    "test:watch": "export NODE_ENV='dev' && jest --watch",
    "test:coverage": "export NODE_ENV='dev' && jest --coverage",
    "test:gov": "export CONFIG='waterbox' && ts-node src/scripts/createCalendar.ts && ts-node src/index.ts",
    "start:api": "export CONSOLE_LOG=on && node ./dist/api/index.js",
    "dev:api": "export CONSOLE_LOG=on && export LOG_LEVEL=silly && nodemon ./src/api/index.ts",
    "build:api": "tsc src/api/index.ts --outDir ./dist --resolveJsonModule --esModuleInterop --skipLibCheck --lib es2021 && cp -r ./src/config/ ./dist/config/ && cp -r ./src/transactions/abi ./dist/transactions/abi",
    "dolt-client": "source .env && mysql -h$DOLT_HOST -u$DOLT_USER -p$DOLT_PASSWORD",
    "dolt-superclient": "source .env && dolt sql-client -H$DOLT_HOST -P$DOLT_PORT -u$DOLT_SUPERUSER -p$DOLT_SUPERPASSWORD"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.6.8",
    "@gnosis.pm/safe-core-sdk": "^2.3.2",
    "@gnosis.pm/safe-core-sdk-types": "^1.4.0",
    "@gnosis.pm/safe-ethers-lib": "^1.4.0",
    "@jigglyjams/juice-sdk-v3": "^3.2.2-alpha",
    "@snapshot-labs/snapshot.js": "^0.3.96",
    "@tryfabric/martian": "^1.2.4",
    "axios": "^0.27.2",
    "common-tags": "^1.8.2",
    "cors": "^2.8.5",
    "deepl-node": "^1.3.1",
    "diff": "^5.1.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.9",
    "ethers-multisend": "^2.4.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "graphql-request": "^4.2.0",
    "ics": "^2.35.0",
    "juice-sdk": "^2.1.0-alpha",
    "juice-sdk-v1": "^2.1.0-alpha",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "next-auth": "^4.22.1",
    "node-ical": "^0.15.1",
    "node-schedule": "^2.1.0",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cors": "^2.8.12",
    "@types/diff": "^5.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "@types/node-schedule": "^2.1.0",
    "@types/object-hash": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "openai": "^3.2.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
  }
}
