{
  "name": "nance",
  "version": "1.0.0",
  "description": "automated governance pipeliner",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV='prod' && export CONFIG='juicebox' && ts-node src/index.ts",
    "dev": "export NODE_ENV='dev' && nodemon src/index.ts",
    "test": "export NODE_ENV='dev' && jest",
    "test:watch": "export NODE_ENV='dev' && jest --watch",
    "test:coverage": "export NODE_ENV='dev' && jest --coverage",
    "test:gov": "ts-node src/scripts/createCalendar.ts&& ts-node src/index.ts",
    "start:api": "export CONSOLE_LOG=on && node ./dist/api/index.js",
    "build:api": "tsc src/api/index.ts --outDir ./dist --esModuleInterop --skipLibCheck --lib es2021"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.6.8",
    "@gnosis.pm/safe-core-sdk": "^2.3.2",
    "@gnosis.pm/safe-core-sdk-types": "^1.4.0",
    "@gnosis.pm/safe-ethers-lib": "^1.4.0",
    "@logtail/node": "^0.1.12",
    "@logtail/winston": "^0.1.13",
    "@notionhq/client": "^1.0.4",
    "@pinata/sdk": "^1.1.24",
    "@snapshot-labs/snapshot.js": "^0.3.96",
    "@tryfabric/martian": "^1.2.4",
    "axios": "^0.27.2",
    "common-tags": "^1.8.2",
    "cors": "^2.8.5",
    "deepl-node": "^1.3.1",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.3",
    "express": "^4.18.1",
    "graphql-request": "^4.2.0",
    "ics": "^2.35.0",
    "juice-sdk": "^2.1.0-alpha",
    "juice-sdk-v1": "^2.1.0-alpha",
    "node-ical": "^0.15.1",
    "node-schedule": "^2.1.0",
    "notion-to-md": "^2.3.2",
    "typescript": "^4.8.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
